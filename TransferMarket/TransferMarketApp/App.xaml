                <ResourceDictionary Source="/View/Themes/SliderTheme.xaml"/>
                <ResourceDictionary Source="/View/Themes/TextBoxTheme.xaml"/>
                <ResourceDictionary Source="/View/Themes/Buttons/UnCoverButtonTheme.xaml"/>
                <ResourceDictionary Source="/View/Themes/Buttons/TabButtonTheme.xaml"/>
                <ResourceDictionary Source="/View/Themes/Buttons/RadioButtonTheme.xaml"/>
